{
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "type": "AdaptiveCard",
  "version": "1.0",
  "body": [
    {
      "type": "TextBlock",
      "text": ""
    },
    {
      "type": "TextBlock",
      "text": "Current Players in the Room:"
    },
    {
      "type": "TextBlock",
      "text": "",
